{
  "name": "dep-analyzer",
  "version": "0.1.0",
  "author": "Team WLGF",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlgf/dep-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/wlgf/dep-analyzer/issues"
  },
  "homepage": "https://github.com/wlgf/dep-analyzer#readme",
  "bin": {
    "dep-analyzer": "./dist/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier \"*.ts\" --write && eslint \"*.ts\" --fix",
    "build": "npm run lint && tsc"
  },
  "main": "index.js",
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "commander": "^11.0.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "htmlWhitespaceSensitivity": "css"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}